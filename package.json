{
  "name": "nextra-docs-template",
  "version": "0.0.1",
  "description": "Nextra docs template",
  "scripts": {
    "check-local": "echo $TINA_PUBLIC_IS_LOCAL",
    "dev": "cross-env TINA_PUBLIC_IS_LOCAL=true && npm run check-local && tinacms dev -c \"next dev\"",
    "build": "tinacms build && next build",
    "start": "next start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/et0and/learn-tewahi-next.git"
  },
  "author": "Tom Hackshaw <tom@tomhackshaw.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/et0and/learn-tewahi-next.git/issues"
  },
  "homepage": "https://github.com/et0and/learn-tewahi-next.git#readme",
  "dependencies": {
    "next": "^13.0.6",
    "nextra": "2.0.1",
    "nextra-theme-docs": "2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tinacms": "^1.6.0"
  },
  "devDependencies": {
    "@tinacms/cli": "^1.5.42",
    "@types/node": "18.11.10",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.1",
    "typescript": "^4.9.3"
  }
